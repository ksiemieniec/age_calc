class Parrot:
    def fly(self):
        print("x")
        print("x")
        print("x")
        print("x")
        print("x")
        print("x")


class Sparrow:
    def fly(self):
        print("x")
        print("x")
        print("x")
        print("x")
        print("x")
        print("x")

        #bad habit

