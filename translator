numbers = input("Type a number to convert into word: ")
database = {
    "0": "zero",
    "1": "one",
    "2": "two",
    "3": "three",
    "4": "four",
    "5": "five",
    "6": "six",
    "7": "seven",
    "8": "eight",
    "9": "nine",
}
output = ""
for number in numbers:
    output += database.get(number, "wrong_value") + " "
print(output)
